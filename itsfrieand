<script type="rocketlazyloadscript" id="delayed_modal_scripts" data-rocket-type="text/javascript">
    function rkft_sleep(ms) {
        return new Promise(resolve => setTimeout(resolve, ms));
    }

    var $ = jQuery;
    (function($) {
        const scripts = [
            "https://www.gopetfriendly.com/blog/wp-content/themes/gopetfriendly/assets/js/css_browser_selector.js",
            "https://www.gopetfriendly.com/blog/wp-content/themes/gopetfriendly/assets/js/jquery-ui.min.js",
            "https://www.gopetfriendly.com/assets-front/js/jquery.validate.js",
            "https://www.gopetfriendly.com/blog/wp-content/themes/gopetfriendly/assets/js/bootstrap.min.js",
            "https://www.gopetfriendly.com/blog/wp-content/themes/gopetfriendly/assets/js/nav.js?1684307299",
            "https://www.gopetfriendly.com/assets-front/js/front-js/login.js",
            "https://www.gopetfriendly.com/assets-front/js/additional-methods.min.js",
            "https://www.gopetfriendly.com/assets-front/js/front-js/register.js",
            "https://www.gopetfriendly.com/assets-front/js/front-js/footer.js?ver=1684307299"
        ];
        setTimeout(async function(){
            let body = document.querySelector("body");
            for (let i = 0; i < scripts.length; i++) {
                var script = document.createElement("script");
                script.src = scripts[i];
                script.async = true;
                body.appendChild(script);
                await rkft_sleep(100);
            }
        },5000);
    })(jQuery);
</script>
<script type="rocketlazyloadscript" data-rocket-type="text/javascript">
jQuery(document).ready(function($) {

	getUserTag();

	function getUserTag(){
	    var id = $('#saveID').val();
	    if(id != '' && id!=undefined){
		$.ajax({
		    url: url+'Login/getUserTag',
		    type: "POST",
		    dataType: 'html',
		    data: {id : id},
		    beforeSend: function (xhr){ 
			$('.p-rmf-loader').show();
		    },
		    success: function(data){
			$('.p-rmf-loader').hide();
			$('#badgeArea').html(data);
		    },
		});
	    }
	    else{
		return false;
	    }
	};
	$('body').on('click', '#googleSignIn', function() {
	    var redirect = $('#urlRedirect').val();
	    $.ajax({
		type:'post',
		url:url+'Home/googleLogin',
		data: {
		    locationResirect : redirect,
		},
		dataType:'json',
		success:function(res){
		    console.log(res);
		    window.location.href= res;
		}
	    }); 
	});
	$('body').on('click', '#facebookSignIn', function() {
	    var redirect = $('#urlRedirect').val();
	    $.ajax({
		type:'post',
		url:url+'Home/facebookLogin',
		data: {
		    locationResirect : redirect,
		},
		dataType:'json',
		success:function(res){
		    console.log(res);
		    window.location.href= res;
		}
	    }); 
	});

	$('.userb').click(function(){
		Userback.open();
	});	
});		
</script>
  
</body>
</html>
